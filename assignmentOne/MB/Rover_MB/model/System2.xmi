<?xml version="1.0" encoding="UTF-8"?>
<rover:System
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:rover="http://www.example.org/rover"
    xsi:schemaLocation="http://www.example.org/rover rover.ecore">
  <rover name="RoverTest">
    <component
        xsi:type="rover:Motor"
        name="Frontmotor"/>
    <component
        xsi:type="rover:GPS"
        name="GPSSensor"
        position="//@program.0/@quantity.0"/>
    <component
        xsi:type="rover:Distance"
        name="DistanceSensor"
        length="//@program.0/@quantity.2"/>
    <component
        xsi:type="rover:Light"
        name="Bluelight"
        color="Blue"/>
  </rover>
  <program rover="//@rover.0"
      name="ProgramTest">
    <block>
      <command
          xsi:type="rover:Move"
          outgoingTransition="//@program.0/@block/@transition.0"
          motor="//@rover.0/@component.0"
          length="//@program.0/@quantity.1"
          velocity="//@program.0/@quantity.3"/>
      <command
          xsi:type="rover:Rotate"
          outgoingTransition="//@program.0/@block/@transition.1"
          incomingTransition="//@program.0/@block/@transition.0"
          angle="//@program.0/@quantity.4"/>
      <command
          xsi:type="rover:Wait"
          outgoingTransition="//@program.0/@block/@transition.2"
          incomingTransition="//@program.0/@block/@transition.1"
          time="//@program.0/@quantity.5"/>
      <command
          xsi:type="rover:Terminate"
          incomingTransition="//@program.0/@block/@transition.2"/>
      <transition
          xsi:type="rover:NormalTransition"
          source="//@program.0/@block/@command.0"
          target="//@program.0/@block/@command.1"/>
      <transition
          xsi:type="rover:NormalTransition"
          source="//@program.0/@block/@command.1"
          target="//@program.0/@block/@command.2"/>
      <transition
          xsi:type="rover:NormalTransition"
          source="//@program.0/@block/@command.2"
          target="//@program.0/@block/@command.3"/>
    </block>
    <quantity
        xsi:type="rover:Position"
        X="//@program.0/@quantity.1"
        Y="//@program.0/@quantity.2"/>
    <quantity
        xsi:type="rover:Length"
        value="15.0"/>
    <quantity
        xsi:type="rover:Length"
        value="40.0"/>
    <quantity
        xsi:type="rover:Velocity"
        value="40.0"/>
    <quantity
        xsi:type="rover:Angle"
        value="20.0"/>
    <quantity
        xsi:type="rover:Time"
        value="500.0"
        timeUnit="milliseconds"/>
  </program>
</rover:System>
