    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="OCL_constraint_03" value="self.command -> select(t : Command | t.oclIsTypeOf(Terminate) ) -> size() &lt;= 1"/>
    </eAnnotations>
