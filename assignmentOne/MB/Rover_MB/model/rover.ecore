    <eStructuralFeatures xsi:type="ecore:EReference" name="program" lowerBound="1"
        upperBound="-1" eType="#//Program" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Rover">
